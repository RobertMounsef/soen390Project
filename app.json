{
  "expo": {
    "name": "Campus Guide",
    "slug": "campus-guide",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/logo192.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./public/logo512.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.debuggingdemons.campusguide"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./public/logo192.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.debuggingdemons.campusguide",
      "config": {
        "googleMaps": {
          "apiKey": "${GOOGLE_MAPS_API_KEY}"
        }
      }
    },
    "plugins": [
      "expo-asset"
    ]
  }
}
